{% extends "WebappBlackspanBundle::layout.html.twig" %}
{% block title %}
Modification -{{ parent() }}
{% endblock %}
{% block webappblackspan_body %}
<h3>Modification d'un accès</h3>
<form action="{{ path('blackspan_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }}>
    {{ form_widget(edit_form) }}
    <p>
        <button type="submit">Edit</button>
    </p>
</form>
<ul class="record_actions">
    <li>
        <a href="{{ path('blackspan') }}">
            Retour à la liste
        </a>
    </li>
    <li>
        <form action="{{ path('blackspan_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{%endblock%}